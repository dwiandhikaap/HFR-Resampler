{
    "framerate": 60, 
    "resolution": "UNCHANGED", 
    "fourcc": "mp4v", 
    "cv_colourfix": false,
    "blend_range": 1.0, 
    "blend_mode": "EQUAL",
    "blend_settings": {
        "pyramid": {
            "reverse": false
        },
        "gaussian": {
            "standard_deviation": 2,
            "bound": [0,2]
        },
        "custom_function": {
            "function": "-x**2+1", 
            "bound": [-0.5,1]
        },
        "custom_weight":{
            "weight": [1,3,2,2]
        }
    }
}